{
  "version": 2,
  "builds": [
    {
      "src": "api/*.go",
      "use": "@now/go",
      "config": {
        "includeFiles": [
          "api/mock_data.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/1/vehicles/[^/]+/data",
      "dest": "/api/data.go"
    },
    {
      "src": "/api/1/vehicles/[^/]+/command/door_unlock",
      "dest": "/api/unlock.go"
    },
    {
      "src": "/api/1/vehicles/[^/]+/command/door_lock",
      "dest": "/api/lock.go"
    },
    {
      "src": "/oauth/token",
      "dest": "/api/oauth.go"
    }
  ]
}
